    "int-test": "vitest run --dir int-test",
    "test": "vitest run"
  },
  "dependencies": {
    "@odata2ts/http-client-base": "^0.5.4"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "@types/node": "^20.2.5",
    "jquery": "^3.6.3",
    "rimraf": "^5.0.1",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "jquery": ">1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
