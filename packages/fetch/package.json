{
  "name": "@odata2ts/http-client-fetch",
  "version": "0.6.2",
  "description": "HTTP client based on fetch and consumable by odata2ts",
  "keywords": [
    "http client",
    "fetch",
    "odata",
    "ts",
    "odata2ts"
  ],
  "repository": "git@github.com:odata2ts/http-client.git",
  "license": "MIT",
  "author": "texttechne",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "*.md",
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "check-circular-deps": "madge ./src --extensions ts --circular",
    "clean": "rimraf lib coverage",
    "compile": "tsc",
    "int-test": "vitest run int-test/**",
    "prepublish": "yarn build",
    "test": "vitest run test/**"
  },
  "dependencies": {
    "@odata2ts/http-client-base": "^0.4.2"
  },
  "devDependencies": {
    "@odata2ts/odata-core": "^0.3.7",
    "@types/node": "^17.0.23",
    "rimraf": "^3.0.2",
    "typescript": "5.0.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
